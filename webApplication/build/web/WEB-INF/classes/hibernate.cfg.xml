<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-configuration PUBLIC "-//Hibernate/Hibernate Configuration DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-configuration-3.0.dtd">
<hibernate-configuration>
  <session-factory>
    <property name="hibernate.dialect">org.hibernate.dialect.DerbyDialect</property>
    <property name="hibernate.connection.driver_class">com.mysql.jdbc.Driver</property>
    <property name="hibernate.connection.url">jdbc:mysql://localhost:3306/hitema?zeroDateTimeBehavior=convertToNull</property>
    <property name="hibernate.connection.username">root</property>
    <mapping resource="beans/PorteOuv.hbm.xml"/>
    <mapping resource="beans/NiveauEtude.hbm.xml"/>
    <mapping class="" file="" jar="" package="" resource="beans/Salon.hbm.xml"/>
    <mapping resource="beans/Candidat.hbm.xml"/>
    <mapping resource="beans/INSEE.hbm.xml"/>
  </session-factory>
</hibernate-configuration>
